    Function check(a As Double, b As Double)

        Dim t1 As Double = a Mod Math.PI
        Dim t2 As Double = b Mod Math.PI

        If (t1 < 0) Then t1 += Math.PI
        If (t2 < 0) Then t2 += Math.PI

        If (Math.Abs(t1 - t2) > 0.0000000009) Then
            Console.WriteLine("Fail:" + a + " " + b)
            pass = False
        End If
    End Function


    Function check(r As Double(), a As Double, k As Double, q As Double, l As Double, h As Double, p As Double)

